{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.379725403380-5lopqnh3ma5aomse9qik09402075rvdn"
        }
      ],
      "expo-apple-authentication",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#09090b",
          "image": "./src/assets/icon-ios-20x20@3x.png",
          "dark": {
            "image": "./src/assets/icon-ios-20x20@3x.png",
            "backgroundColor": "#09090b"
          },
          "imageWidth": 42
        }
      ]
    ],
    "name": "Quizzo",
    "slug": "quizzo",
    "icon": "./src/assets/icon-ios-20x20@3x.png",
    "backgroundColor": "#09090b",
    "android": {
      "package": "com.lucasjuste.quizzo"
    },
    "ios": {
      "bundleIdentifier": "com.lucasjuste.quizzo",
      "usesAppleSignIn": true,
      "simulator": "true",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "GIDClientID": "379725403380-5lopqnh3ma5aomse9qik09402075rvdn.apps.googleusercontent.com"
      }
    },
    "assetBundlePatterns": [
      "assets/fonts/*"
    ]
  }
}